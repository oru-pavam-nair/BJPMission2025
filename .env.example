# Kerala Map Standalone - Environment Configuration

# Application Version
VITE_APP_VERSION=1.0.0

# Application Name
VITE_APP_NAME="Kerala Map Standalone"

# Enable Service Worker in development (optional)
# VITE_ENABLE_SW=true

# API Configuration (if needed in future)
# VITE_API_BASE_URL=https://api.example.com

# Supabase Configuration (if needed)
# VITE_SUPABASE_URL=your_supabase_url
# VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Debug Mode (development only)
# VITE_DEBUG_MODE=true

# Performance Monitoring
# VITE_ENABLE_PERFORMANCE_MONITORING=true

# Cache Configuration
# VITE_CACHE_DURATION=300000

# Map Configuration
# VITE_DEFAULT_MAP_ZOOM=7
# VITE_MAP_CENTER_LAT=10.8505
# VITE_MAP_CENTER_LNG=76.2711